            "hash": "da1d42000fa551f4243cf1bec06b554761e5eb7a1f81d5d514a1ba0fbe0060fb"
        },
        "64bit": {
            "url": "https://ci.appveyor.com/api/projects/punesemu/puNES/artifacts/punes64.wip.d3d9.zip",
            "hash": "28dfc67db93fcd030fae7b7c4131df139083281a775edc311bfb12c4ca8302a1"
        }
    },
    "installer": {
        "script": "Move-Item \"$dir\\punes.exe\" \"$dir\\punes_p.exe\""
    },
    "bin": [
        [
            "punes_p.exe",
            "punes-wip-d3d9"
        ]
    ],
    "shortcuts": [
        [
            "punes_p.exe",
            "puNES\\puNES WIP (D3D9)"
        ]
    ],
    "persist": [
        "bios",
        "cheat",
        "diff",
        "pgs",
        "prb",
        "save",
        "screenshot",
        "shp"
    ],
    "checkver": {
        "url": "https://ci.appveyor.com/api/projects/punesemu/punes/",
        "regex": "\"version\":\"(.*?)\".*\"commitId\""
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://ci.appveyor.com/api/projects/punesemu/puNES/artifacts/punes32.wip.d3d9.zip"
            },
            "64bit": {
                "url": "https://ci.appveyor.com/api/projects/punesemu/puNES/artifacts/punes64.wip.d3d9.zip"
            }
        }
    },
    "post_install": [
        "'puNES.cfg', 'input.cfg', 'recent.cfg' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\$_.bak\")) {",
        "        New-Item -ItemType File \"$dir\\$_\" | Out-Null",
        "    } else {",
        "        Copy-Item \"$persist_dir\\$_.bak\" \"$dir\\$_\" -Force",
        "    }",
        "}"
    ],
    "uninstaller": {
        "script": [
            "'puNES.cfg', 'input.cfg', 'recent.cfg' | ForEach-Object {",
            "    Copy-Item \"$dir\\$_\" \"$persist_dir\\$_.bak\" -Force",
            "}"
        ]
    },
    "notes": "Configuration files cannot be persisted, but will be retained during the update"
}
