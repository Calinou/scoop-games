{
    "version": "nightly",
    "description": "Official launcher for the game Hytale",
    "homepage": "https://hytale.com",
    "license": {
        "identifier": "Proprietary",
        "url": "https://hytale.com/terms-of-service"
    },
    "url": "https://launcher.hytale.com/builds/release/windows/amd64/hytale-launcher-installer-latest.exe#/.7z",
    "pre_install": "Remove-Item \"$Dir\\`$PLUGINSDIR\" -Force -Recurse",
    "shortcuts": [
        [
            "hytale-launcher.exe",
            "Hytale"
        ]
    ],
    "notes": [
        "Delete 'HKLM\\SOFTWARE\\Hypixel Studios' from the Windows Registry.",
        "This will force Hytale to use default directories for the game & launcher."
    ]
}
